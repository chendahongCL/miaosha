<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.miaosha.product.mapper.ProductInfoMapper">
    <insert id="saveProductDetail" parameterType="com.youfan.entity.ProductDetail">
        <selectKey keyProperty="id" order="AFTER" resultType="java.lang.Integer">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into productdetail(productid,productpalce,productbrand,productdecsrption,productWeight,productdetailPcitureUrl,specificationAndPack) value(#{productid},#{productpalce},#{productbrand},#{productdecsrption},#{productWeight},#{productdetailPcitureUrl},#{specificationAndPack})
    </insert>

</mapper>
